<mvc:View controllerName="com.comparisonpattern.Test_ComparisonPattern.controller.Worklist" xmlns:mvc="sap.ui.core.mvc"
	xmlns:unified="sap.ui.unified" xmlns="sap.m" xmlns:fb="sap.ui.comp.filterbar" xmlns:gt="sap.ui.table" xmlns:f="sap.ui.layout.form"
	xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:semantic="sap.m.semantic">
	<semantic:FullscreenPage id="page" title="{i18n>worklistTitle}">
		<semantic:content>
			<HBox class="sapUiSmallMargin">
				<Label text="Date:" design="Bold" class="sapUiTinyMarginTop sapUiTinyMarginEnd"/>
				<DatePicker id="datePickerID" valueFormat="yyyy-MM-dd" displayFormat="dd-MM-yyyy" value="{worklistView>/dDate}"
					maxDate="{worklistView>/dMaxDate}" change="onDateChange"/>
				<!--<DatePicker id="datePickerID1" value="{ path:'worklistView>/dDate', type: 'sap.ui.model.type.Date', formatOptions: { style: 'long' } }"-->
				<!--	parseError="alert('Parse error')" validationError="alert('Validation error')"/>-->
				<!--<DateTimePicker id="myDTP" width="15rem"-->
				<!--	value="{ path:'worklistView>/dDate', type: 'sap.ui.model.type.DateTime', formatOptions: { style: 'short/short' } }"-->
				<!--	parseError="alert('Parse error')" validationError="alert('Validation error')"/>-->
			</HBox>
		</semantic:content>
		<semantic:customFooterContent>
			<ToolbarSpacer/>
			<Button text="{i18n>OpenDetails}" type="Emphasized" press="onPressOpenDetails"/>
		</semantic:customFooterContent>
	</semantic:FullscreenPage>
</mvc:View>